{
  "name": "vuex-arcsecond-module-generator",
  "version": "1.2.9",
  "description": "A Vuex Plugin",
  "author": "CeÃÅdric Foellmi <cedric@onekilopars.ec>",
  "main": "dist/vuex-arcsecond-module-generator.common.js",
  "module": "dist/vuex-arcsecond-module-generator.esm.js",
  "browser": "dist/vuex-arcsecond-module-generator.js",
  "unpkg": "dist/vuex-arcsecond-module-generator.js",
  "style": "dist/vuex-arcsecond-module-generator.css",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "node build/build.js",
    "build:dll": "webpack --progress --config build/webpack.config.dll.js",
    "lint": "yon run lint:js",
    "lint:js": "eslint --ext js --ext jsx --ext vue src test/**/*.spec.js test/*.js build",
    "lint:js:fix": "yon run lint:js -- --fix",
    "lint:staged": "lint-staged",
    "pretest": "yon run lint",
    "test": "jest",
    "dev": "webpack-dashboard -- webpack-dev-server --config build/webpack.config.dev.js --open",
    "dev:coverage": "cross-env BABEL_ENV=test karma start test/karma.conf.js",
    "prepublish": "yon run build"
  },
  "publishConfig": {
    "registry": "http://localhost:4873"
  },
  "lint-staged": {
    "*.{vue,jsx,js}": [
      "eslint --fix"
    ],
    "*.{vue,css}": [
      "stylefmt",
      "stylelint"
    ]
  },
  "pre-commit": "lint:staged",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.12",
    "add-asset-html-webpack-plugin": "^2.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.0",
    "babel-jest": "^22.4.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.4.0",
    "babel-preset-env": "^1.7.0",
    "buble": "^0.15.2",
    "clean-css": "^4.0.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-vue": "^2.0.0",
    "eslint-plugin-vue": "^2.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^22.4.0",
    "lint-staged": "^3.4.0",
    "mkdirp": "^0.5.1",
    "nock": "^9.1.9",
    "postcss": "^6.0.0",
    "postcss-cssnext": "^2.10.0",
    "pre-commit": "^1.2.0",
    "rimraf": "^2.6.0",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.0",
    "rollup-plugin-jsx": "^1.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.4.1",
    "rollup-plugin-replace": "^1.1.0",
    "rollup-plugin-vue": "^2.3.0",
    "style-loader": "^0.17.0",
    "stylefmt": "^5.3.0",
    "stylelint": "^7.10.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-processor-html": "^1.0.0",
    "uglify-js": "^3.0.0",
    "uppercamelcase": "^3.0.0",
    "vue": "^2.3.0",
    "vue-jest": "^2.1.0",
    "vue-loader": "^12.0.0",
    "vue-resource": "^1.4.0",
    "vue-template-compiler": "^2.3.0",
    "vuex": "^3.0.1",
    "webpack": "^2.5.0",
    "webpack-bundle-analyzer": "^2.4.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.4.0",
    "webpack-merge": "^4.0.0",
    "yarn-or-npm": "^2.0.0"
  },
  "peerDependencies": {
    "lodash": "^4.17.4",
    "vue": "^2.5.13"
  },
  "dllPlugin": {
    "name": "vuePluginTemplateDeps",
    "include": [
      "html-entities",
      "vue/dist/vue.js",
      "core-js/library",
      "url",
      "sockjs-client",
      "vue-style-loader/lib/addStylesClient.js",
      "events",
      "ansi-html",
      "style-loader/addStyles.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onekiloparsec/vuex-arcsecond-module-generator.git"
  },
  "bugs": {
    "url": "https://github.com/onekiloparsec/vuex-arcsecond-module-generator/issues"
  },
  "homepage": "https://github.com/onekiloparsec/vuex-arcsecond-module-generator#readme",
  "license": {
    "type": "MIT",
    "url": "http://www.opensource.org/licenses/mit-license.php"
  },
  "dependencies": {
    "vue-resource": "^1.5.0",
    "vuex": "^3.0.1"
  }
}
